{
  "name": "boldr",
  "version": "0.6.0-alpha.4",
  "description": "Your dreams are bold. Your thoughts are bold. So why shouldn't your CMS be a little Boldr?",
  "main": "build/boldrCMS/server/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boldr/boldr.git"
  },
  "keywords": [
    "react",
    "universal",
    "javascript",
    "express",
    "webpack",
    "postgres",
    "node",
    "cms",
    "redux"
  ],
  "author": "Steven Truesdell <steven@strues.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/boldr/boldr/issues"
  },
  "homepage": "https://github.com/boldr/boldr#readme",
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=3.0.0",
    "yarn": "^0.14.0"
  },
  "betterScripts": {
    "dev:client": {
      "command": "node ./tools/development",
      "env": {
        "DEBUG": "boldr:*",
        "NODE_ENV": "development",
        "BABEL_ENV": "development"
      }
    },
    "dev:api": {
      "command": "nodemon ./src/api/index.js",
      "env": {
        "DEBUG": "boldr:*",
        "NODE_ENV": "development",
        "BABEL_ENV": "api",
        "redis__uri": "redis://127.0.0.1:6379/1",
        "postgres__uri": "postgres://postgres:password@localhost:5432/boldr",
        "API_PORT": 2121
      }
    }
  },
  "scripts": {
    "analyze": "node tools/scripts/analyze",
    "apidoc": "apidoc -i src/api -o public/apidoc && apidoc-markdown -p public/apidoc -o docs/apidoc.md",
    "babel": "NODE_ENV=production babel --ignore node_modules",
    "build": "node tools/scripts/build",
    "build:api": "NODE_ENV=production BABEL_ENV=api npm run babel -- ./src/api -d ./build/boldrAPI",
    "clean": "node tools/scripts/clean",
    "cleanup": "rimraf .happypack && rimraf build/",
    "dev": "npm-run-all --parallel dev:api dev:client",
    "dev:client": "better-npm-run dev:client",
    "dev:api": "better-npm-run dev:api",
    "flow": "node ./tools/scripts/flow",
    "flow:defs": "flow-typed install -f 0.34 --overwrite",
    "knex": "./node_modules/.bin/knex --knexfile src/api/config/knexfile.js",
    "lint": "./node_modules/.bin/eslint src",
    "lint:fix": "npm run lint -- --fix",
    "migrate": "./node_modules/.bin/knex --knexfile src/api/config/knexfile.js migrate:latest",
    "migrate:prod": "./node_modules/.bin/knex --knexfile boldrAPI/config/knexfile.js migrate:latest",
    "seed": "./node_modules/.bin/knex --knexfile src/api/config/knexfile.js seed:run",
    "seed:prod": "./node_modules/.bin/knex --knexfile boldrAPI/config/knexfile.js seed:run",
    "start:cms": "NODE_ENV=production pm2 start build/boldrCMS/server/index.js --name boldrSSR -e logs/boldrSSR-error.log",
    "start:api": "NODE_ENV=production postgres__uri=postgres://postgres:password@localhost:5432/boldr pm2 start boldrAPI/index.js --name boldrAPI -e logs/boldrAPI-error.log",
    "test:ci": "NODE_ENV=test CI=true jest -w 2",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch=all"
  },
  "jest": {
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tools/test/styleMock.js",
      "^.+\\.(css|scss)$": "<rootDir>/tools/test/styleMock.js"
    },
    "collectCoverage": true,
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testPathDirs": [
      "<rootDir>/src/cms",
      "<rootDir>/src/api"
    ],
    "setupFiles": [
      "<rootDir>/tools/test/setup.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(tools|docs|node_modules|bin|flow-typed|public)/"
    ],
    "preprocessorIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageReporters": [
      "lcov",
      "text",
      "text-summary"
    ]
  },
  "apidoc": {
    "title": "Boldr API Documentation",
    "url": "https://boldr.io/api/v1"
  },
  "dependencies": {
    "app-root-path": "^2.0.1",
