{
  "name": "boldr-ui",
  "version": "0.0.1",
  "description": "UI components for BoldrCMS",
  "main": "lib/index.js",
  "scripts": {
    "build": "webpack --config internal/webpack/webpack.prod.config.js --progress --colors",
    "build:es": "NODE_ENV=production babel --ignore=__tests__,__mocks__,src/**/*.test.js ./src -d ./lib",
    "build:test": "echo \"nothing\" && exit 0",
    "format": "prettier-eslint --write \"src/**/*.js\"",
    "start": "cross-env NODE_ENV=development UV_THREADPOOL_SIZE=100 node ./server",
    "test": "NODE_ENV=test jest -w 4 -u --config=internal/jest/jest.browser.json --forceExit",
    "test:ci": "NODE_ENV=test jest -w 4 -u --config=internal/jest/jest.browser.json --forceExit"
  },
  "files": [
    "src/",
    "dist/",
    "lib/",
    "LICENSE"
  ],
  "keywords": [
    "react",
    "boldr"
  ],
  "author": "Steven Truesdell <steven@strues.io>",
  "license": "MIT",
  "dependencies": {
    "react-md": "^1.0.11",
    "styled-components": "^1.4.4"
  },
